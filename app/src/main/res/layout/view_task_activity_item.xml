<?xml version="1.0" encoding="utf-8"?>
<com.balysv.materialripple.MaterialRippleLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingTop="@dimen/task_activity_span_item_padding"
    android:paddingBottom="@dimen/task_activity_span_item_padding"
    app:mrl_rippleHover="true"
    app:mrl_rippleInAdapter="false"
    app:mrl_rippleColor="@color/white"
    app:mrl_rippleAlpha="1">
    <com.simbirsoft.timeactivity.ui.views.TaskActivityItemView
        android:id="@+id/taskActivityItemView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
</com.balysv.materialripple.MaterialRippleLayout>